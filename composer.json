{
  "name": "dba_hh/statista-challenge",
  "description": "statista challenge",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Patrick Nehls",
      "email": "Bewerbung.statista@nehls.de"
    }
  ],
  "require": {
    "php": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "7.3"
    }
  },
  "autoload": {
    "psr-4": {
      "dba_hh\\statista_challenge\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "dba_hh\\statista_challenge_tests\\": "tests/"
    }
  },
  "prefer-stable": true
}